	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "vue-jest",
			".*\\.(js)$": "babel-jest"
		},
		"collectCoverage": true,
		"collectCoverageFrom": [
			"<rootDir>/src/pages/index/src/**/*.{js,vue}",
			"!**/node_modules/**",
			"!**/useful-builder/**",
			"!**/personalInfoEmpty.js",
			"!<rootDir>/src/pages/index/src/{index,vuetify,router}.js"
		],
		"moduleNameMapper": {
			"\\.(css|less)$": "<rootDir>/test/assetsTransformer.js",
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/assetsTransformer.js",
			"^jsDir(.*)$": "<rootDir>/src/assets/js$1",
			"^imagesDir(.*)$": "<rootDir>/src/assets/images$1",
			"^cssDir(.*)$": "<rootDir>/src/assets/css$1"
		},
		"testEnvironment": "jsdom",
